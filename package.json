{
  "name": "qubic",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "set AD_CONTROLLER=xxx&&node ./bin/www",
    "build-css": "stylus style --out public/stylesheets",
    "watch-css": "nodemon -e styl -x \"npm run build-css\"",
    "build-js": "browserify client/main.js | uglifyjs -o public/javascript/main.js",
    "watch-js": "watchify client/main.js -o public/javascript/main.js -v",
    "watch-assets": "npm run watch-css & npm run watch-js",
    "watch-server": "nodemon --ignore style/ --ignore public/ --ignore client/ --ignore test/ ./bin/www --ignore node_modules/",
    "watch": "npm run watch-assets & npm run watch-server",
    "watch-win": "start npm run watch-css & start npm run watch-js & start npm run watch-server",
    "build": "npm run build-js & npm run build-css",
    "test": "mocha",
    "dev": "DEBUG=qube-web:* NODE_ENV=development npm run watch",
    "dev-win": "set DEBUG=qube-web:*&& set NODE_ENV=development&& set AD_CONTROLLER=xxx&& npm run watch-win",
    "install-windows-service": "winser -i",
    "uninstall-windows-service": "winser -r"
  },
  "author": {
    "name": "Benjamin Norrington",
    "email": "ben@norrington.net"
  },
  "license": "Proprietary",
  "dependencies": {
    "axis": "^0.3.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.12.0",
    "browserchannel": "^2.0.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.3.4",
    "cookie-session": "^1.1.0",
    "express": "~4.12.2",
    "express-ntlm": "^2.1.4",
    "express-session": "^1.11.1",
    "font-awesome": "^4.4.0",
    "friar": "mavxg/friar",
    "hat": "0.0.3",
    "jade": "~1.9.2",
    "kbpgp": "^2.0.35",
    "livedb": "git://github.com/mavxg/livedb",
    "livedb-mongo": "^0.4.1",
    "method-override": "^2.3.5",
    "moment": "^2.10.6",
    "mongodb": "^1.4.35",
    "mongoose": "^4.0.1",
    "morgan": "~1.5.1",
    "openpgp": "^1.2.0",
    "ot-sexpr": "mavxg/ot-sexpr",
    "passport": "^0.2.1",
    "passport-custom": "^1.0.1",
    "passport-facebook": "^2.0.0",
    "passport-github": "^0.1.5",
    "passport-google-oauth": "^0.2.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.3",
    "serve-favicon": "~2.2.0",
    "share": "^0.7.19",
    "slate0": "git://github.com/mavxg/ot-sexpr",
    "slatejs": "git://github.com/mavxg/slatejs",
    "stylus": "0.42.3",
    "winser": "1.0.0"
  },
  "devDependencies": {
    "browserify": "^9.0.3",
    "copyfiles": "^0.2.1",
    "mocha": "^2.2.1",
    "nodemon": "^1.3.7",
    "parallelshell": "^2.0.0",
    "uglifyjs": "^2.4.10",
    "watchify": "^2.4.0"
  }
}
